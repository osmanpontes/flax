{
  "name": "flax-counter",
  "version": "0.0.0",
  "description": "Flax - Counter Example",
  "main": "src/app.js",
  "scripts": {
    "start": "watchify src/app.js -o build/app.js -v -d",
    "vendor": "browserify -r react -r react-dom > build/vendor.js",
    "production": "cross-env NODE_ENV=production browserify -x react -x react-dom src/app.js > build/app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "flux",
    "flax",
    "react",
    "counter"
  ],
  "repository": "https://github.com/osmanpontes/flax",
  "author": "Samuel Timb√≥ <samuelmtimbo@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "react": "^0.14.6",
    "react-dom": "^0.14.6"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "cross-env": "^1.0.7",
    "envify": "3.4.0",
    "uglify-js": "^2.6.1",
    "watchify": "^3.7.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ],
      "envify"
    ]
  }
}
